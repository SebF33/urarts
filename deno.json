{
  "tasks": {
    "build": "deno run -A dev.ts build",
    "migrate": "deno run -A utils/db_migration.ts",
    "migrate_up": "deno run -A utils/db_migration.ts --up",
    "migrate_down": "deno run -A utils/db_migration.ts --down",
    "preview": "deno run -A main.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.4.3/",
    "@/": "./",
    "@components/": "./components/",
    "@islands/": "./islands/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@twind": "./utils/twind.ts",
    "@utils/": "./utils/",
    "any": "https://deno.land/std@0.159.0/encoding/_yaml/utils.ts",
    "chartjs": "https://cdn.skypack.dev/chart.js@4.3.0",
    "ky": "https://esm.sh/ky@v0.33.3",
    "kysely": "https://esm.sh/kysely@0.26.0",
    "preact": "https://esm.sh/preact@10.17.1",
    "preact/debug": "https://esm.sh/preact@10.17.1/debug",
    "preact/hooks": "https://esm.sh/preact@10.17.1/hooks",
    "preact/jsx-runtime": "https://esm.sh/preact@10.17.1/jsx-runtime",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "sqlite": "https://deno.land/x/sqlite@v3.7.0/mod.ts",
    "tippyjs": "https://cdn.skypack.dev/tippy.js@6.3.7/dist/tippy.esm.js",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/css": "https://esm.sh/twind@0.16.19/css",
    "twind/sheets": "https://esm.sh/twind@0.16.19/sheets",
    "preact/": "https://esm.sh/preact@10.17.1/",
    "twind/": "https://esm.sh/twind@0.16.19/"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}